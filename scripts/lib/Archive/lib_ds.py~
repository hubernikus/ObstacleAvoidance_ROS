from numpy import np

def linearAttractor_const(x, x0 = 'default', velConst=0.3, distSlow=):
    # change initial value for n dimensions
    # TODO -- constant velocity // maximum velocity
    return x0-x

    if len(x.shape) ==1:
        dim = x.shape
        M=1
    else:
        dim,M = x.shape

    if x0=='default':
        x0 = dim*[0]
    xd = - (x-np.tile(x0,M))
    
    return xd
 
